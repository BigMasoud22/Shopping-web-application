@if (TempData["AddAdminSuccess"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <script type="text/javascript">
                                $(function(){
                    Swal.fire({
          position: 'center',
          icon: 'success',
          title: '@TempData["AddAdminSuccess"]',
          showConfirmButton: false,
          timer: 3000
        })
                                });
    </script>
}
@if (TempData["AddAdminFaild"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <script type="text/javascript">
